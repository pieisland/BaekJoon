int prime[10000];
int arr[10000];
int num;
int s, e;

void eratos()
{
	for (int i = 2; i <= num; i++)
	{
		if (prime[i] == 0)
			continue;
		for (int j = i + i; j <= num; j += i)
		{
			prime[j] = 0;
		}
	}
}

int bfs()
{
	memset(arr, 0, sizeof(arr));

	queue<int> q;
	q.push(s);
	arr[s] = 1;

	while (!q.empty())
	{
		int cnum = q.front();
		q.pop();

		if (cnum == e)
			return arr[cnum] - 1;

		int digit[4] = { cnum / 1000, (cnum / 100) % 10, (cnum / 10) % 10, cnum % 10 };

		for (int i = 0; i < 4; i++)
		{
			if (i == 0)
			{

			}
		}

	}
}

int main(){

	int t;
	cin >> t;

	while (t--)
	{
		cin >> s >> e;
		int result = bfs();
		if (result == -1)
			cout << "impossible" << endl;
		else
			cout << result << endl;
	}

	return 0;
}
