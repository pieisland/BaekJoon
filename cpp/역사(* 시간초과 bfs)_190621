#include<cstdio>
#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
#include<cstring>
#include<queue>
using namespace std;

/*
19.06.21.ê¸ˆ.
*/
int n, k;
int history[401][401];

void floyd()
{
	for(int k=1;k<=n;k++)
		for(int i=1;i<=n;i++)
			for (int j = 1; j <= n; j++)
			{
				if (i == j || j == k || i == k)
					continue;
				if(history[i][j]==0){
				if (history[i][k] == -1 && history[k][j] == -1)
					history[i][j] = -1;

				else if (history[i][k] == 1 && history[k][j] == 1)
					history[i][j] = 1;
			}
}

int main(){

	cin >> n >> k;
	
	int a, b;
	for (int i = 0; i < k; i++)
	{
		cin >> a >> b;
		history[a][b] = -1;
		history[b][a] = 1;
	}

	floyd();
	int s;
	cin >> s;
	for (int i = 0; i < s; i++)
	{
		cin >> a >> b;
		cout << history[a][b] <<"\n" ;
	}

	return 0;
}
