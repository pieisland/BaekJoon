#include<cstdio>
#include<iostream>
#include<vector>
#include<algorithm>
#include<string>
#include<cstring>
#include<queue>
#include<math.h>
using namespace std;

int a[501];
int t[501];

int main()
{
	int n;
	cin>>n;
	vector<int> vec[501];

	queue<int> q;

	for(int i=0;i<n;i++)
	{
		int time;
		int b;
		cin >> time;//아니 근데 이게 걸리는 시간이라서 저렇게 못하네요.
		
		//건물을 짓는 데 걸리는 시간을 저장한다.
		t[i+1] = time;
		while(1)
		{
			cin>>b;
			if(b==-1)
				break;
			else
			{
				//연결해줍니다.
				a[b]++;//진입 차수를 증가시켜준다.
				vec[i+1].push_back(b);
			}
		}
	}

	for(int i=1;i<=n;i++)
	{
		if(a[i]==0) //진입 차수가 0인 친구들은 넣어준다.
			q.push(i);
	}

	while(!q.empty())
	{
		//
		int id = q.front();
		q.pop();
		cout<<id<<endl;
		for(int i=0;i<vec[id].size();i++)
		{
			if(--a[vec[id][i]]==0)
				q.push(vec[id][i]);
		}
	}

	return 0;
}
